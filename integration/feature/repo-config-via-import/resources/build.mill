package build

import mill._
import com.grack.nanojson.JsonParser
import com.grack.nanojson.JsonObject

// Line under test: add the given repo as Maven repo to the configured repositories


def parseJson(jsonString: String) = Task.Command {
  val jsonObj = JsonParser.`object`().from(jsonString)

  jsonObj.entries.forEach { v =>
    println(s"Key: ${v.key}, Value: ${v.value}")
  }
}