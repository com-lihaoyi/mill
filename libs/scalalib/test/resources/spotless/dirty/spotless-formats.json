[
  {
    "includes": [
      "glob:**/EndWithNewline"
    ],
    "steps": [
      {
        "$type": "EndWithNewline"
      }
    ]
  },
  {
    "includes": [
      "glob:**/FenceWithin"
    ],
    "steps": [
      {
        "$type": "FenceWithin",
        "fence": {
          "name": "java template",
          "pattern": [
            "<java>",
            "</java>"
          ]
        },
        "steps": [
          {
            "$type": "PalantirJavaFormat"
          }
        ]
      },
      {
        "$type": "FenceWithin",
        "fence": {
          "name": "scala template",
          "pattern": [
            "<scala>",
            "</scala>"
          ]
        },
        "steps": [
          {
            "$type": "ScalaFmt",
            "configFile": null
          }
        ]
      }
    ]
  },
  {
    "includes": [
      "glob:**/FenceToggle"
    ],
    "steps": [
      {
        "$type": "FenceToggle",
        "steps": [
          {
            "$type": "PalantirJavaFormat"
          }
        ]
      }
    ]
  },
  {
    "includes": [
      "glob:**/Indent"
    ],
    "steps": [
      {
        "$type": "Indent"
      }
    ]
  },
  {
    "includes": [
      "glob:**/LicenseHeader"
    ],
    "steps": [
      {
        "$type": "LicenseHeader"
      }
    ]
  },
  {
    "includes": [
      "glob:**/ReplaceRegex"
    ],
    "steps": [
      {
        "$type": "ReplaceRegex",
        "name": "Greetings to Mars",
        "regex": "(hello) w[a-z]{3}d",
        "replacement": "$1 mars"
      }
    ]
  },
  {
    "includes": [
      "glob:**/Replace"
    ],
    "steps": [
      {
        "$type": "Replace",
        "name": "Greetings to Mars",
        "target": "World",
        "replacement": "Mars"
      }
    ]
  },
  {
    "includes": [
      "glob:**/TrimTrailingWhitespace"
    ],
    "steps": [
      {
        "$type": "TrimTrailingWhitespace"
      }
    ]
  },
  {
    "includes": [
      "glob:**/CleanthatJava"
    ],
    "steps": [
      {
        "$type": "CleanthatJava",
        "sourceJdkVersion": "11",
        "mutators": [
          "LiteralsFirstInComparisons"
        ]
      }
    ]
  },
  {
    "includes": [
      "glob:**/FormatAnnotations"
    ],
    "steps": [
      {
        "$type": "FormatAnnotations"
      }
    ]
  },
  {
    "includes": [
      "glob:**/GoogleJavaFormat"
    ],
    "steps": [
      {
        "$type": "GoogleJavaFormat"
      }
    ]
  },
  {
    "includes": [
      "glob:**/ImportOrder"
    ],
    "steps": [
      {
        "$type": "ImportOrder"
      }
    ]
  },
  {
    "includes": [
      "glob:**/PalantirJavaFormat"
    ],
    "steps": [
      {
        "$type": "PalantirJavaFormat"
      }
    ]
  },
  {
    "includes": [
      "glob:**/RemoveUnusedImports"
    ],
    "steps": [
      {
        "$type": "RemoveUnusedImports"
      }
    ]
  },
  {
    "includes": [
      "glob:**/Ktfmt"
    ],
    "steps": [
      {
        "$type": "Ktfmt"
      }
    ]
  },
  {
    "includes": [
      "glob:**/ScalaFmt"
    ],
    "steps": [
      {
        "$type": "ScalaFmt"
      }
    ]
  }
]